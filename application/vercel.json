{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/routes/(.*)",
      "dest": "/routes/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
